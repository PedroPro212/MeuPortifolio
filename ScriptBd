CREATE DATABASE Portifolio;
USE Portifolio;

CREATE TABLE Orcamento(
    id int PRIMARY KEY AUTO_INCREMENT NOT NULL,
    Nome varchar(200) NOT NULL,
    Email varchar(300) NOT NULL,
    Tel varchar(11),
    Escolha varchar(50),
    DataHora DATETIME
);

